[Unit]
Description=Redirect ports {{proxy_second_ports_ssh|join(',')}} to SSH service
After=network.target

[Service]
User=root
Group=root
Type=oneshot 
RemainAfterExit=yes

[Service]
User=root
Group=root
WorkingDirectory={{proxy_ssh_script|dirname}}
ExecStart={{proxy_ssh_script}} start
ExecStop={{proxy_ssh_script}} stop

[Install]
WantedBy=multi-user.target